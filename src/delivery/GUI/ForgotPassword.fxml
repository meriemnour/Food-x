<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="500.0" style="-fx-background-color: #28252e;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="delivery.GUI.ForgotPasswordController">
   <children>
      <Label layoutX="142.0" layoutY="90.0" text="Forgot Password" textFill="#0fea88">
         <font>
            <Font name="Times New Roman Bold" size="31.0" />
         </font>
      </Label>
      <TextField fx:id="tfemail_tel" layoutX="176.0" layoutY="153.0" promptText="email or username" />
      <PasswordField fx:id="pfnew_password" layoutX="176.0" layoutY="200.0" promptText="new password" />
      <PasswordField fx:id="pfconfirm" layoutX="176.0" layoutY="244.0" promptText="confirm password" />
      <Button fx:id="btsearch" layoutX="365.0" layoutY="153.0" mnemonicParsing="false" onAction="#search" style="-fx-background-color: #0fea88;" text="Search" textFill="WHITE">
         <font>
            <Font name="Times New Roman Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="btupdate" layoutX="219.0" layoutY="302.0" mnemonicParsing="false" onAction="#update" style="-fx-background-color: #0fea88;" text="Update" textFill="WHITE">
         <font>
            <Font name="Times New Roman Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="btlogin" layoutX="225.0" layoutY="344.0" mnemonicParsing="false" onAction="#btlogin" style="-fx-background-color: #0fea88;" text="login" textFill="WHITE">
         <font>
            <Font name="Times New Roman Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
